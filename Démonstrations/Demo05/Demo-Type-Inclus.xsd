<?xml version="1.1" encoding="UTF-8" standalone="yes"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:element name="month">
        <xsd:simpleType>
            <xsd:restriction base="xsd:positiveInteger">
                <xsd:maxInclusive value="12"/>
            </xsd:restriction>
        </xsd:simpleType>
    </xsd:element>
</xsd:schema>